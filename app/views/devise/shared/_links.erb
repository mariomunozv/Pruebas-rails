<%- if controller_name != 'sessions' %>
  <%= link_to "Ingresar", new_session_path(resource_name), :class => "btn btn-link" %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registro", client_register_path, :class => "btn btn-link" %>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Olvidó su contraseña?", new_password_path(resource_name), :class => "btn btn-link" %>
<% end -%>
<!--
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "No recibió las instrucciones de recuperación?", new_confirmation_path(resource_name), :class => "btn btn-link" %>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "No recibió las instrucciones para desbloquear?", new_unlock_path(resource_name), :class => "btn btn-link" %>
<% end -%>
-->
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Registrado como #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider), :class => "btn btn-link" %>
  <% end -%>
<% end -%>
